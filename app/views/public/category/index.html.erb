<div class-"container">

  <%= @category.name %>の投稿一覧
  
  <table class="table">
    
    <tr>
      <th>タイトル</th>
      <th>本文</th>
    </tr>
    
    <% @posts.each do |post| %>
      <tr>
        <td><%= link_to post.title, post_path(post) %></td>
        <td><%= post.body %></td>
      </tr>
    <% end %>
    
  </table>

</div>